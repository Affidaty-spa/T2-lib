{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["./src/main.ts"],
    "tsconfig": "./tsconfig.json",
    "out": "docs",
    "emit": "docs",
    "name": "T2Lib Documentation",
    "theme": "default",
    "darkHighlightTheme": "dark-plus",
    "excludePrivate": true,
    "externalPattern": [
        "**/node_modules/**"
    ],
    "excludeExternals": true
}